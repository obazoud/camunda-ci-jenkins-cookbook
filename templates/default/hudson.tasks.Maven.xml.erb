<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Maven_-DescriptorImpl>
  <installations>
    <% @mavens.each do |mvn| %>
    <hudson.tasks.Maven_-MavenInstallation>
      <name><%= mvn[:name] -%></name>
      <properties>
        <hudson.tools.InstallSourceProperty>
          <installers>
            <hudson.tasks.Maven_-MavenInstaller>
              <id><%= mvn[:version] -%></id>
            </hudson.tasks.Maven_-MavenInstaller>
          </installers>
        </hudson.tools.InstallSourceProperty>
      </properties>
    </hudson.tasks.Maven_-MavenInstallation>
    <% end %>
  </installations>
</hudson.tasks.Maven_-DescriptorImpl>